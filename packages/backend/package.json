{
  "name": "@bitpull/backend",
  "version": "1.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=production ts-node --transpile-only -r tsconfig-paths/register ./src/index.ts",
    "dev": "concurrently \"ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules ./src/index.ts \"",
    "debug": "concurrently \"ts-node-dev -r tsconfig-paths/register --ignore-watch node_modules  --inspect -- ./src/index.ts\" \"sleep 10 && yarn typegen\"",
    "db:migrate": "migrate up",
    "db:migrate:make": "migrate create",
    "db:migrate:rollback": "migrate down",
    "ts-node": "ts-node -r tsconfig-paths/register src/index.ts",
    "typegen": "graphql-codegen",
    "tsc": "tsc --skipLibCheck && rm -rf ./build",
    "lint": "eslint --ext .ts 'src' --fix",
    "lint:staged": "lint-staged",
    "test": "jest --maxWorkers=2",
    "test:watch": "jest --watch"
  },
  "dependencies": {
