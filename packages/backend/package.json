    "graphql-type-json": "^0.3.1",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "migrate-mongoose-typescript": "^3.3.4",
    "mongoose": "^5.9.7",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "qs": "^6.9.3",
