    "@material-ui/pickers": "^v3.2.10",
    "apollo-boost": "^0.4.7",
    "apollo-link-context": "^1.0.19",
    "apollo-link-ws": "^1.0.19",
    "classnames": "^2.2.6",
    "d3-hierarchy": "^1.1.9",
    "d3-selection": "^1.4.1",
    "d3-zoom": "^1.8.3",
    "date-fns": "^2.11.1",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "helmet": "^3.22.0",
    "lodash.debounce": "^4.0.8",
    "notistack": "^0.9.9",
    "query-string": "^6.11.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^5.3.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^6.1.1",
    "recharts": "^1.8.5",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "subscriptions-transport-ws": "^0.9.16",
    "typescript": "3.8.3",
    "uuidv4": "^6.0.7",
    "yup": "^0.28.3"
